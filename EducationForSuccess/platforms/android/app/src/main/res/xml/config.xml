<?xml version='1.0' encoding='utf-8'?>
<widget id="net.educationforsuccess.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>EducationForSuccess</name>
    <description>
        Mobile app for the Education For Success non-profit organization.
    </description>
    <author email="shahrahuld3@gmail.com" href="http://rahul-shah.ml">
        Rahul Shah
    </author>
    <content src="main.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
